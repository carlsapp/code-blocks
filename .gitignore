pip-log.txt                   # Installer Logs
pip-delete-this-directory.txt #

# Unit test / coverage reports
htmlcov/
.tox/
.coverage
.coverage.*
.cache
nosetests.xml
coverage.xml
*,cover
.hypothesis/

